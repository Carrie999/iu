{
	"name": "iu",
	"displayName": "iu 2021",
	"description": "A two-color-ball-inspired colour theme to satisfy your dreams",
	"version": "0.1.0",
	"author": "yajing pang",
	"publisher": "Carrie999",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Carrie999/twoColorBall"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"keywords": [
		"spring",
		"soft",
		"iu"
	],
	"categories": [
		"Themes"
	],
	"activationEvents": [
		"*",
		"onCommand:synthwave84.enableNeon",
		"onCommand:synthwave84.whatsNew"
	],
	"main": "./src/extension.js",
	"contributes": {
		"themes": [
			{
				"label": "iu",
				"uiTheme": "vs-dark",
				"path": "./themes/synthwave-color-theme.json"
			}
		],
		"commands": [
			{
				"command": "iu.enableNeon",
				"title": "Synthwave '84: Enable Neon Dreams"
			},
			{
				"command": "iu.disableNeon",
				"title": "iu: Disable Neon Dreams"
			},
			{
				"command": "iu.whatsNew",
				"title": "iu: What's New"
			}
		],
		"configuration": {
			"title": "iu 2020",
			"properties": {
				"iu.brightness": {
					"type": "number",
					"default": 0.45,
					"description": "Set the brightness value for the glow effect. 0.0 is fully transparent and 1.0 is fully bright"
				},
				"iu.disableGlow": {
					"type": "boolean",
					"default": false,
					"description": "Disable the glow effect"
				}
			}
		}
	},
	"dependencies": {
		"semver": "^7.1.1"
	},
	"__metadata": {
		"publisherDisplayName": "Carrie999"
	}
}